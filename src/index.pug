html(lang="en")

  head
    meta(charset="utf-8")
    title Your Website
    meta(name="description", content="Your Website")
    link(rel="stylesheet",
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css",
      integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T",
      crossorigin="anonymous")
    link(rel="stylesheet" type="text/css" href="css/styles.css")
    
  body

  nav(class="navbar navbar-expand-lg navbar-dark bg-dark")

    a(class="navbar-brand", href="#") Your Website
    button(class="navbar-toggler",
      type="button",
      data-toggle="collapse",
      data-target="#navbarSupportedContent",
      aria-controls="navbarSupportedContent",
      aria-expanded="false",
      aria-label="Toggle navigation")
      span(class="navbar-toggler-icon")

    div(class="collapse navbar-collapse", id="navbarSupportedContent")

      ul(class="navbar-nav mr-auto")

        li(class="nav-item active")
          a(class="nav-link", href="#") Home
            span(class="sr-only") current

        li(class="nav-item dropdown")
          a(class="nav-link dropdown-toggle",
            href="#",
            id="navbarDropdown",
            role="button",
            data-toggle="dropdown",
            aria-haspopup="true",
            aria-expanded="false") Dropdown
          div(class="dropdown-menu", aria-labelledby="navbarDropdown")
            a(class="dropdown-item", href="#") Action
            a(class="dropdown-item" href="#") Another action
            div(class="dropdown-divider")
              a(class="dropdown-item", href="#") Something else here 3

        li(class="nav-item")
          a(class="nav-link disabled", href="#") Disabled
        
  div(class="row")
    div(class="col bg-success text-white") Success!
    div(class="col bg-warning text-dark") Warning!
    div(class="col bg-danger text-white") Failure!

  div(class="row")
    div(class="col")
      div(class="customBox")

  script(src="https://code.jquery.com/jquery-3.4.1.slim.min.js",
